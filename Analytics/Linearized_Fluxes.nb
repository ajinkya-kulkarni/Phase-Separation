Cell[BoxData[
 RowBox[{
  RowBox[{"sol1DInside", "[", "x_", "]"}], "=", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"c", "[", "x", "]"}], "/.", " ", 
     RowBox[{"First", "@", 
      RowBox[{"DSolve", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"0", "==", 
           RowBox[{
            RowBox[{"Diff", "*", 
             RowBox[{"Laplacian", "[", 
              RowBox[{
               RowBox[{"c", "[", "x", "]"}], ",", 
               RowBox[{"{", "x", "}"}], ",", "\"\<Cartesian\>\""}], "]"}]}], 
            "+", "ScZeroIn", "-", 
            RowBox[{"k", "*", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"c", "[", "x", "]"}], "-", "cZeroIn"}], ")"}]}]}]}], 
